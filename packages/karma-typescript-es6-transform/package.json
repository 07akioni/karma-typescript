{
  "name": "karma-typescript-es6-transform",
  "version": "5.0.1",
  "description": "Plugin for transforming ES2015 javascript code to to ES5 syntax.",
  "files": [
    "dist/**"
  ],
  "main": "dist/transform.js",
  "keywords": [
    "babel",
    "ES2015",
    "ES6",
    "karma-typescript",
    "typescript"
  ],
  "author": "monounity",
  "contributors": [
    "erikbarke"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/monounity/karma-typescript.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "test": "nyc --require ts-node/register tape src/*.spec.ts | faucet"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "reporter": [
      "html"
    ],
    "tempDir": "./coverage/.nyc_output"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "acorn-walk": "^7.1.1",
    "log4js": "^6.1.2",
    "magic-string": "^0.25.7"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.7",
    "@types/karma": "^4.4.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/sinon": "^9.0.0",
    "@types/tape": "^4.2.34",
    "acorn": "^7.1.1",
    "faucet": "0.0.1",
    "i": "^0.3.6",
    "karma-typescript": "5.0.1",
    "nyc": "^15.0.1",
    "postcss": "^7.0.27",
    "sinon": "^9.0.1",
    "tape": "^4.13.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "latest"
  }
}
